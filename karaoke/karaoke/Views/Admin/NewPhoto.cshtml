@model karaoke.Models.Photo
@{
    ViewBag.Title = "NewPhoto";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Add Photo</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Baslik)
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.BlogImage)
    @Html.TextAreaFor(x => x.BlogImage, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Add</button>
</form>
